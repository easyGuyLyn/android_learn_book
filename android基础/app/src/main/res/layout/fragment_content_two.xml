<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:name="com.dawoo.gamebox.adapter.HomeAdapter.ContentAdapter.TwoItemFragment"
    tools:context="com.dawoo.gamebox.adapter.HomeAdapter.ContentAdapter.TwoItemFragment"
    tools:listitem="@layout/recycleview_item_homefragment_content_view_list__item"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:background="@color/white"
        android:paddingEnd="@dimen/activity_horizontal_margin"
        android:paddingStart="@dimen/activity_horizontal_margin"
        app:layoutManager="LinearLayoutManager"
        />
</FrameLayout>


